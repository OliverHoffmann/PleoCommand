'                         Java2D Disposer' daemon:true  alive:true  interrupted:false priority:10 state:WAITING
'                              TimerQueue' daemon:true  alive:true  interrupted:false priority: 5 state:TIMED_WAITING
'                                AWT-XAWT' daemon:true  alive:true  interrupted:false priority: 6 state:RUNNABLE
'                            AWT-Shutdown' daemon:false alive:true  interrupted:false priority: 5 state:WAITING
'                        AWT-EventQueue-0' daemon:false alive:true  interrupted:false priority: 6 state:WAITING
'                         LogTable-Update' daemon:true  alive:true  interrupted:false priority: 6 state:WAITING
'             Pipe Thumbnail Update Timer' daemon:true  alive:true  interrupted:false priority: 6 state:WAITING
'                           DestroyJavaVM' daemon:false alive:true  interrupted:false priority: 5 state:RUNNABLE
